//- sprint/index.pug
extends ../_includes/layout.pug

block head
  title Studio Sprint
  block stylesandscripts
    link(rel="stylesheet" type="text/css" href="../_styles/style.css")
    link(id="favicon" rel="shortcut icon" type="image/png" href="../_assets/img/favicon.png")

//- block meta pulls in here

block content

  main
    .main-wrapper
      .grid-three.grid-lines
        .grid-column-one
        .grid-column-two
        .grid-column-three

      .grid-two-one.head-container
        div
          h1 <a href="/">&larr;</a> <span>Join the MDS List </span> and get notified when new videos are released.
        div
          .instruction-tag.instruction-tag--center
            p Fill this out
          .instruction-tag--line.instruction-tag--line-vertical
          include ../_includes/form.pug

    block foot
      include ../_includes/foot.pug

  block scripts
    script(src='/_scripts/index.js')



